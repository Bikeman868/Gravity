                                { "condition": "{path[1]} = ui" }
                            ]
                        },
                        {
                            "routeTo": "C"
                        }
                    ]
                }
            ],
            "pages": [
                { "name": "B" }
            ],
            "roundRobin": [
                { "name": "C", "outputs": [ "D", "E", "F" ] }
            ],
            "servers": [
                { "name": "D" },
                { "name": "E" },
                { "name": "F" }
            ],
            "responses": [ ],
            "stickySessions": [ ],
            "transforms": [ ]
        },
        "pages": {
        }
    }
