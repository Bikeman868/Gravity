                "indented": true
            }
        }
    },
    "gravity": {
        "listener": {
            "endpoints": [
                { "ipAddress": "*", "port": 0, "node": "A" }
            ]
        },
        "nodeGraph": {
            "routers": [
                {
                    "name": "A",
                    "outputs": [
                        {
                            "routeTo": "B",
